{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/**/*.js",
          "shared/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel.js"
    }
  ]
}